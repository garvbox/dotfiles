
# Git Abbrevs
abbr -a gco "git checkout"
abbr -a gcb "git checkout -b"
abbr -a gfa "git fetch --all --prune"
abbr -a gca "git commit --verbose --all"
abbr -a gd "git diff"
abbr -a gl "git pull"
abbr -a gp "git push"
abbr -a gst "git status"

# Editor
alias vim="nvim"

# General Shell Usage
# Rusty tools :-)
alias ls="exa"
alias cat="bat"
alias grep="rg"
